        "grunt-contrib-concat": "*",
        "grunt-contrib-uglify": "*",
        "grunt-contrib-copy": "*",
        "grunt-contrib-clean": "*",
        "grunt-contrib-connect": "*",
        "grunt-contrib-watch": "*",
        "load-grunt-tasks": "*",
        "grunt-contrib-cssmin": "*",
        "grunt-aws-s3": "*",
        "grunt-notify": "*",
