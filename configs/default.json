{
    "session": {
        "enabled": true,
        "options": {
            "secret": "expR3ssS3cR3TASD:Fwfk%$^$%&*&",
            "cookie": {
                "secure": true,
                "maxAge": 999999999999,
                "signed": true,
                "path": "/"
            },
            "resave": true,
            "saveUninitialized": true
        }
    },
    "passport": {
        "enabled": true
    },
    "cache": {
        "redis": {
            "host": "127.0.0.1",
            "port": 6379
        },
        "memcache": {}
    },
    "l12n": {
        "throwOnMissingTranslation": false
    },

    "extensions": {
        "Basic HTTP Parsers": {
            "module": "twee-extensions/http/parsers"
        },

        "Responses In Formats": {
            "module": "twee-extensions/http/responses"
        },

        "Twee Winston Logger": {
            "module": "twee-extensions/logging/winston",
            "disabled": true
        },

        "Twee Custom Headers": {
            "module": "twee-extensions/headers/requested-with"
        },

        "Static Files Serving": {
            "module": "twee-extensions/http/static"
        },

        "Basic Morgan Logger": {
            "module": "twee-extensions/logging/basic"
        },

        "View Engine": {
            "module": "twee-extensions/view/engine/swig"
        }
    },

    "options": {
        "staticFiles": {
            "directory": "public"
        },

        "logging": {
            "file": "var/logs/access.log"
        },

        "favicon": {
            "file": "/public/favicon.ico"
        },

        "bodyParser": {
            "urlencoded": {
                "extended": false
            }
        },

        "viewEngine": {
            "swig": {
                "engineExtension": "html",
                "options": {
                    "cache": null
                }
            },
            "jade": {
                "engineExtension": "jade",
                "options": {
                    "pretty": true,
                    "compileDebug": false
                }
            },
            "disabled": false
        }
    }
}
